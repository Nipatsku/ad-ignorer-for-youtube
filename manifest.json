{
    "manifest_version": 2,
    "name": "Ad-Ignore",
    "version": "0.1.1",
    "icons": {
      "128": "icon_128.png",
      "48": "icon_48.png"
    },
    "permissions": [
      "*://*.youtube.com/watch*"
    ],
    "content_scripts": [
        {
          "matches": [
            "*://*.youtube.com/watch*"
          ],
          "js": ["content.js"],
          "css": ["style.css"]
        }
      ],
    "web_accessible_resources": [
      "assets/*"
    ]
  }